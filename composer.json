{
  "name": "ecce/craft-dev-plugin",
  "description": "Quick Setup For a Craft Plugin Utalising Docker and Docker Compose",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "Unlicense",
  "homepage": "https://ecce.uk/",
  "type": "project",
  "support": {
    "email": "support@ecce.uk"
  },
  "require": {
    "composer/composer": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "Setup\\": "craft/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "Setup\\Events::postCreateProject",
      "docker-compose run web setup/security-key"
    ]
  }
}
